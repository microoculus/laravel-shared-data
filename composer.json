{
    "name": "microoculus/laravel-shared-data",
    "description": "Package for sharing data from Laravel to JavaScript",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Microoculus\\SharedData\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Microoculus\\SharedData\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --colors=always"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Microoculus\\SharedData\\Providers\\SharedDataServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "authors": [
        {
            "name": "Microoculus",
            "email": "microoculus@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
